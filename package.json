  "scripts": {
    "dev": "nodemon",
    "start": "NODE_ENV=production nodemon --exec ts-node --project tsconfig.server.json src/app.ts -e ts",
    "test:feature": "jest -c jest.config.feature.js --passWithNoTests --runInBand --forceExit --detectOpenHandles",
    "test:unit": "jest -c jest.config.unit.js --findRelatedTests --coverage src/services/**/*.ts",
    "test": "yarn test:unit",
    "entity:create": "ts-node ./node_modules/typeorm/cli.js entity:create -n",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "db:seed": "ts-node ./databases/index.ts seed",
    "db:job": "ts-node ./databases/index.ts job",
    "prelint": "tsc --noEmit",
    "lint": "eslint --fix --ext .js,.ts ./src",
    "deploy:dev": "DEPLOY_TARGET=dev yarn deploy",
    "deploy:release": "DEPLOY_TARGET=release yarn deploy",
    "deploy": "git tag $DEPLOY_TARGET-$npm_package_version && git push --tags && git push && echo \"Successfully released version $npm_package_version to $DEPLOY_TARGET!\""
  },
  "resolutions": {
    "fs-capacitor": "5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prelint && lint-staged && yarn test:feature"
    }
  }
}
